# Contributor:
pkgname=junit
pkgver=4.10
arch=(i686 x86_64)
pkgrel=1
pkgdesc="junit."
url="http://junit.com/"
license=('BSD')
source=(https://github.com/downloads/KentBeck/junit/$pkgname$pkgver.zip)

build() {
  cd $srcdir/
  # remove zipfile
  rm $pkgname$pkgver.zip
  mkdir $pkgdir/opt/
  cp -R $srcdir/* $pkgdir/opt/junit
}

md5sums=('5daf0a18e4f2dbb4a34fbdad455d6182')
