# Contributor:
pkgname=junit
pkgver=4.9b3
arch=(i686 x86_64)
pkgrel=1
pkgdesc="junit."
url="http://junit.com/"
license=('BSD')
source=(https://s3.amazonaws.com/rstudio-buildtools/junit-4.9b3.jar)

build() {
  cd $srcdir/
  mkdir -p $pkgdir/opt/junit
  cp -R $srcdir/* $pkgdir/opt/junit
}

md5sums=('542e08e4fac8819735331966c5c16016')
